// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Aluno {
  id    String @id
  nome  String
  email String @unique
  turma String

  @@map("aluno")
}

model Professor {
  id         String @id
  nome       String
  email      String @unique
  disciplina String

  @@map("professor")
}
