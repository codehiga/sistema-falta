generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Aluno {
  id    String @id
  nome  String
  email String @unique
  turma String

  @@map("aluno")
}

model Professor {
  id         String @id
  nome       String
  email      String @unique

  @@map("professor")
}

model Disciplina {
  id         String @id
  nome       String @unique

  @@map("disciplina")
}